{
  "expectation_suite_name": "compliance_w2g_conditional_suite",
  "meta": {
    "great_expectations_version": "0.18.0",
    "notes": "Conditional validation for W-2G filings - validates gambling winnings thresholds",
    "domain": "Regulatory Compliance - W-2G",
    "compliance_requirements": [
      "IRS Form W-2G requirements",
      "General threshold: $600",
      "Slot machine threshold: $1,200 (reduced by wager)",
      "Bingo/Keno threshold: $1,500",
      "Poker tournament threshold: $5,000"
    ],
    "usage_note": "Apply this suite to W2G-filtered data only"
  },
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 0,
        "max_value": null
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "filing_type",
        "value_set": ["W2G"]
      },
      "meta": {
        "notes": "This suite is for W-2G filings only"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "amount",
        "min_value": 600,
        "max_value": null
      },
      "meta": {
        "notes": "W-2G required for gambling winnings >= $600 (IRS requirement)"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "game_type"
      },
      "meta": {
        "notes": "Game type required for W-2G (determines threshold)"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "transaction_date"
      },
      "meta": {
        "notes": "Date of winning required for W-2G"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "player_id"
      },
      "meta": {
        "notes": "Winner identification required for W-2G"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "wager_amount",
        "min_value": 0,
        "max_value": null
      },
      "meta": {
        "notes": "Wager amount for net winnings calculation"
      }
    }
  ]
}
